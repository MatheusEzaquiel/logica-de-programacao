Algoritmo "EX041"

var num, denom, divisao: real

Inicio

    //ENTRADA
    escreva("Insira o numerador da fração: ")
    leia(num)
    
    escreva("Insira o denominador da fração: ")
    leia(denom)
    
    //PROCESSO
    divisao <- num / denom
    
    //SAÍDA
    escreva("O número decimal equivalente a fração", num ," /",denom," é: ",divisao:5:3)

Fimalgoritmo