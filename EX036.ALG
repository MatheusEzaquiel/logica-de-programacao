Algoritmo "EX036"

var grauC, grauF: real

Inicio

    //ENTRADA
    escreva("Informe a temperatura em graus Celsius (°C): ")
    leia(grauC)
    
    //PROCESSO
    grauF <- (9 * grauC + 160) / 5
    
    //SAÍDA
    escreva("A temperatura em fahrenheit correspondente é: ", grauF, "ºF")
    
Fimalgoritmo