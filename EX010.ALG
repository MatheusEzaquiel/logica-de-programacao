Algoritmo "EX010"

var num1, num2, produto: inteiro

Inicio

    //ENTRADA
    escreva("Informe um número: ")
    leia(num1)
    
    escreva("Informe outro número: ")
    leia(num2)
    
    //PROCESSO
    produto <- num1 * num2
    
    //SAÍDA
    escreva("O produto entre os números ", num1, " e ", num2, " é igual a ",produto)

Fimalgoritmo