Algoritmo "EX013"

var num1, num2: inteiro

Inicio

    //ENTRADA
    escreva("Informe o primeiro número: ")
    leia(num1)

    escreva("Informe o segundo número: ")
    leia(num2)
    
    //SAÍDA
    escreval("Dividendo: ", num1)
    escreval("Divisor: ", num2)
    escreval("Quociente: ", num1 div num2)
    escreval("Resto: ", num1 % num2)

Fimalgoritmo